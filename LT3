    <script>
        let a = +prompt("nhập số thứ nhất :");
        let b = +prompt("nhập số thứ hai :");
        let tong = a+b
        function showResult(a,b){
            if(a>b){
                alert(a + "lớn hơn" + b)
            }else{
                alert("tổng là " + tong)
            }
        } showResult(a,b)

    </script>
